<section id={{CVTypes.fieldwork.id}} class=cv-section>

  {{> cv/section-header/index CVTypes.fieldwork.header}}

  <ol>
    {{#each CVItems.fieldwork}}
      <li id='{{cvid}}' class=cv-item>
        {{> cv/date/index }}
        <p class='cv-item-content description'><strong>{{location}}.</strong> {{md description 'inline'}}</strong></p>
      </li>
    {{/each}}
  </ol>

</section>
