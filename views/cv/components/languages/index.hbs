<section id=languages class=cv-section>

  {{> cv/section-header/index 'Languages'}}

  <ol>
    {{#each CVItems.languages}}
      <li id='{{cvid}}' class=cv-item>
        <p class=language><a href='{{links.info}}'>{{title}} (<span class=unicode lang='{{iso}}'>{{autonym}}</span>)</a></p>
        <p class=competency>{{competency}}</p>
      </li>
    {{/each}}
  </ol>

</section>
